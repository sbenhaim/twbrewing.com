body {
	background-image: url( "assets/background-slate.png" );
	text-align: center;
	font-size: 0;
	color: white;
	font-family: "Helvetica", Arial, sans-serif;
}


#header {
	display: inline-block;
	@include width( 12 );

	#tabs {
		float: left;
		@include horizontal-nav;

		ul {
			height: 45px;
			display: table;

			li {
				text-align: center;
				vertical-align: bottom;
				display: table-cell;
				padding-left: 15px;
				padding-bottom: 6px;

				a {
					text-shadow: 0px 1px 0px black;
					font-size: 20px;
					font-family: 'OstrichSansBlack', sans-serif;
				}

				a.active {
					text-shadow: 0px 1px 0px white;
					color: #4f4d4d;
				}
			}

			li:first-child {
				padding-left: 0;
			}
			
			li#twb-tab {
				width: 245px;
				height: 100%;
			}

			li#twb-tab.active {
				background-image: url( "assets/twb-tab-active.png" );
			}

		}
	}

	#contact {
		float: right;
		text-align: right;
		padding-top: 21px;

		a {
			color: #c8c6b4;
			text-shadow: 0px 1px 0px black;
			font-size: 17px;
			font-family: 'OstrichSansBlack', sans-serif;
		}
	}
}

#primary-nav {
	background-image: url( "assets/background-sand.png" );
	@include horizontal-nav;
	padding: 12px 0 12px;

	ul {
		@include width(12);

		li {
			@include width(2);
		}

		li#logo-cog {
			@include width(4);
		}

	}
}

#banner {
	background-image: url( "assets/background-granite.png" );
}


#banner.beers {
	background-image: url( "assets/background-granite-beers.png" );
}

#beers {
	padding-bottom: 24px;
	background-image: url( "assets/beers.png" );
	background-repeat: no-repeat;
	background-position: top center;
	width: 100%;
}

#lower {
	padding-bottom: 68px;
	background-image: url( "assets/background-shale.png" );
	div {
		display: inline-block;
	}

	h2 {
		font-weight: normal;
		text-shadow: 0px 1px 1px white;
		font-size: 25px;
		font-family: 'OstrichSansBlack', sans-serif;
		color: #918f8b;
		padding-bottom: 24px;
	}

	img.h2 {
		padding-bottom: 24px;
	}

	
	img.v-div {
		padding-bottom: 32px;
	}

	#lower-nav {
		text-align: center;

		.left, .middle, .right {
			@include width(4);
		}
	}
}

#footer {

	padding: 30px 0 30px;

	img {
		padding-bottom: 15px;
	}

	div {
		@include width( 4 );
		display: inline-block;
		font-size: $font-size;
	}

	li {
		list-style: none;
		padding: 6px;
	}
}

#content {
	background: none;
	background-color: white;
	padding: 65px 0 85px 0;

	div {
		display: inline-block;
		vertical-align: top;
	}

	p {
		padding-bottom: 1.5em;
	}

	#left {
		@include width( 8 );

		font-size: $font-size;
		color: $content-color;
		text-align: left;

		h1 {
			color: $headline-color;
			font-family: "OstrichSansLight";
			font-size: 48px;
			padding-bottom: 23px;
		}

		h2 {
			color: #605f5e;
			font-family: "OstrichSansBlack";
			font-size: 28px;
			padding-bottom: 13px;
		}
	}

	/* #divider { */
	/* 	@include width( 1 ); */
	/* 	height: 300px; */
	/* 	background-image: url( 'assets/divider-background.png' ); */
	/* 	background-position: center; */
	/* 	background-repeat: repeat-y; */
	/* } */

	#right {
		@include width( 4 );

		#recipe-header {
			background-color: $sidebar-background-color;
		}

		#recipe {
			padding: 25px 23px 32px 23px;
			text-align: left;
			background-color: #918f8b;
			background-image: url( 'assets/sidebar-bottom.png' );
			background-repeat: no-repeat;
			background-position: bottom;
			font-size: 12px;
			width: 224px;
			border-top: 1px solid gray;
			font-weight: bold;
			text-shadow: 0px 1px 0px black;
			line-height: 1.8em;
		}

		h2 {
			font-family: "Helvetica", Arial, sans-serif;
			color: white;
			font-size: 18px;
			padding-bottom: 5px;
		}

		h3 {
			font-family: "OstrichSansBlack";
			padding-top: 20px;
			color: #ecdcaa;
			font-size: 16px;
		}
	}
}
